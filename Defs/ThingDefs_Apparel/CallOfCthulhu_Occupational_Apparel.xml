<ThingDefs>

<!-- Angler -->

	<ThingDef ParentName="CallOfCthulhuApparelMakeableBase">
		<defName>ROM_ApparelRaincoat</defName>
		<description>Water resistant coat often used by fishermen.
		</description>
		<label>rain coat</label>
		<graphicData>
			<texPath>Apparel/Display/ROMRainCoat</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costStuffCount>150</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>20000</WorkToMake>
			<ArmorRating_Blunt>0.12</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.08</ArmorRating_Sharp>
			<Insulation_Cold>-10</Insulation_Cold>
			<Insulation_Heat>-2</Insulation_Heat>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.005</MoveSpeed>
			<WorkSpeedGlobal>0.005</WorkSpeedGlobal>
		</equippedStatOffsets>

		<generateCommonality>0.1</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<worngraphicPath>Apparel/Bodywear_Clothing/Raincoat/ROMRainCoat</worngraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
				<li>ROM_RainCoat</li>
			</tags>
			<defaultOutfitTags>
				<li>ROM_RainCoat</li>
			</defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
			<researchPrerequisite>ROM_OccupationClothing</researchPrerequisite>
		</recipeMaker>
	</ThingDef>


