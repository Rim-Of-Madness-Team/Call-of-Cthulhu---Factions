<Defs>

<!-- Angler -->

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>ROM_ApparelRaincoat</defName>
		<description>Water resistant coat often used by fishermen.</description>
		<label>rain coat</label>
		<graphicData>
			<texPath>Apparel/Display/ROMRainCoat</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
	  <color>(26, 215, 51)</color>
    </graphicData>
    <costStuffCount>150</costStuffCount>
	<thingSetMakerTags>
	  <li>NoRecolor</li>
	</thingSetMakerTags>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<statBases>
		    <Mass>2.2</Mass>
			<MaxHitPoints>120</MaxHitPoints>
			<WorkToMake>20000</WorkToMake>
            <StuffEffectMultiplierArmor>0.3</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0.5</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0.5</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>2.5</EquipDelay>
		</statBases>
		<generateCommonality>0.1</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Apparel/Bodywear_Clothing/Raincoat/ROMRainCoat</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
				<li>ROM_RainCoat</li>
			</tags>
			<defaultOutfitTags>
				<li>ROM_RainCoat</li>
			</defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
			<researchPrerequisite>ROM_OccupationClothing</researchPrerequisite>
		</recipeMaker>
    <comps>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
	</ThingDef>

